@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.SearhResultsPage>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@inject Umbraco.Cms.Core.IPublishedContentQuery publishedContentQuery
@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor


@{
	Layout = "MasterTemplate.cshtml";
	string searchTerm = Context?.Request?.Query["query"].ToString()!;
	<!--var query = HttpContextAccessor.HttpContext.Request.Query["q"];-->	


<main class="mb-4">
	<div class="container px-5 px-lg-4">
		<div class"row gx-4 gx-lg-5 justify-content-center">
			<div class="col-md-15 col-lg-9 col-xl-7">		
				<h5>Din sökning: @searchTerm</h5>			
				<partial name="Search/_UmbracoSearch" model="(searchTerm, Umbraco)"/>
			</div>
		</div>
	</div>
	</main>
}





